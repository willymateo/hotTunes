{
  "name": "nightlytunes",
  "private": true,
  "version": "0.0.1",
  "description": "The best music libary at the moment",
  "scripts": {
    "start:dev": "vite --config ./config/vite.config.js",
    "build": "vite build --config ./config/vite.config.js",
    "preview": "vite preview --config ./config/vite.config.js",
    "prettier:check": "prettier --check . --ignore-path ./config/.prettierignore",
    "prettier:write": "prettier --write . --ignore-path ./config/.prettierignore --ignore-unknown",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@reduxjs/toolkit": "^1.8.1",
    "@vime/core": "^5.3.1",
    "@vime/react": "^5.3.1",
    "axios": "^0.26.1",
    "framer-motion": "^6.3.0",
    "iconoir-react": "^4.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "prettier": "2.6.2",
    "vite": "^2.9.0"
  }
